<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>login_validation</servlet-name>
        <servlet-class>login_validation</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>add_events</servlet-name>
        <servlet-class>add_events</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>events</servlet-name>
        <servlet-class>events</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Delete</servlet-name>
        <servlet-class>Delete</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>filter</servlet-name>
        <servlet-class>filter</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Year</servlet-name>
        <servlet-class>Year</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login_validation</servlet-name>
        <url-pattern>/login_validation</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>add_events</servlet-name>
        <url-pattern>/add_events</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>events</servlet-name>
        <url-pattern>/events</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Delete</servlet-name>
        <url-pattern>/Delete</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>filter</servlet-name>
        <url-pattern>/filter</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Year</servlet-name>
        <url-pattern>/Year</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
